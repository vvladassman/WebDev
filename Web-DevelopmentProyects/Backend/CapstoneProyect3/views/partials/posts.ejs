<div class="PostPage">
    <h1 class="title" id="PostsTitle">Posts.</h1>
    <div id="PostsContainer">
        <%if(locals.posts){ %>
        <% posts.forEach(post => { %>
            
            <div><a href="/Read?title=<%= post.title %>"><%= post.title %></a></div> 
       
            
        <% }); %>
        <% } %>
    </div>
</div>